{
	"name": "json-in-details",
	"version": "0.6.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/diogenesofweb/json-in-details.git"
	},
	"description": "Simple JSON viewer",
	"keywords": [
		"json",
		"json-display",
		"json-view",
		"json-viewer"
	],
	"homepage": "https://github.com/diogenesofweb/json-in-details",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"prepub": "npm version minor && npm run package",
		"pub": "npm publish && git push && git push --tags",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./styles.css": "./dist/styles.css"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@kazkadien/svelte": "^0.23.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-cloudflare": "^2.3.0",
		"@sveltejs/kit": "^1.20.1",
		"@sveltejs/package": "^2.0.2",
		"idb": "^7.1.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.12",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"vite": "^4.3.9"
	},
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"type": "module"
}
